{
  "compilerOptions": {
  
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    }
},
"include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}